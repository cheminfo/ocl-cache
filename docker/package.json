{
  "name": "ocl-cache",
  "version": "0.0.0",
  "description": "Cache molecule directed information in sqlite3",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "dev": "npm run tsc && DEBUG=* node ./lib/server.js",
    "eslint": "eslint src --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "processSmiles": "npm run tsc-cjs && DEBUG=* node scripts/processSmiles",
    "processSDF": "npm run tsc-cjs && DEBUG=* node scripts/processSDF",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "start": "npm run tsc && node ./lib/server.js",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "vitest run --globals --coverage",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/ocl-cache.git"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/ocl-cache/issues"
  },
  "homepage": "https://github.com/cheminfo/ocl-cache#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@vitest/coverage-v8": "^3.0.7",
    "eslint": "^9.21.0",
    "eslint-config-cheminfo-typescript": "^17.0.0",
    "prettier": "^3.5.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.7"
  },
  "dependencies": {
    "@fastify/cors": "^10.1.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/debug": "^4.1.12",
    "better-sqlite3": "^11.8.1",
    "debug": "^4.4.0",
    "fastify": "^5.2.1",
    "file-collection": "^1.0.0",
    "mass-tools": "^2.2.15",
    "openchemlib": "^8.18.1",
    "openchemlib-utils": "^6.5.3",
    "piscina": "^4.8.0",
    "sdf-parser": "^6.0.1"
  }
}
