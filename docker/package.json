{
  "name": "ocl-cache",
  "version": "0.0.0",
  "description": "Cache molecule directed information in sqlite3",
  "main": "./src/index.ts",
  "type": "module",
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib ",
    "dev": "npm run tsc && DEBUG=* node ./lib/server.js",
    "eslint": "eslint src --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "processSmiles": "tsc && DEBUG=* node scripts/processSmiles.mjs",
    "processSDF": "tsc && DEBUG=* node scripts/processSDF",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "start": "npm run tsc && node ./lib/server.js",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "node --test --experimental-strip-types --disable-warning=ExperimentalWarning",
    "tsc": "npm run clean && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/ocl-cache.git"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/ocl-cache/issues"
  },
  "homepage": "https://github.com/cheminfo/ocl-cache#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/expect": "^29.7.0",
    "@types/node": "^22.13.5",
    "@vitest/coverage-v8": "^3.0.7",
    "eslint": "^9.21.0",
    "eslint-config-cheminfo-typescript": "^17.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@fastify/cors": "^10.1.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/debug": "^4.1.12",
    "better-sqlite3": "^11.8.1",
    "bson": "^6.10.3",
    "fastify": "^5.2.1",
    "file-collection": "^1.0.0",
    "mass-tools": "^2.2.15",
    "openchemlib": "^8.18.1",
    "openchemlib-utils": "^6.5.3",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "piscina": "^4.8.0",
    "postgrator": "^8.0.0",
    "sdf-parser": "^7.0.3"
  }
}
