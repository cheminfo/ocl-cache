{
  "name": "ocl-cache",
  "private": true,
  "version": "1.1.1",
  "license": "MIT",
  "description": "Cache molecule directed information in sqlite3",
  "author": "Luc Patiny",
  "type": "module",
  "scripts": {
    "check-types": "tsc --noEmit",
    "dev": "node --watch ./src/server.ts",
    "eslint": "eslint . --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "processSmiles": "node src/scripts/processSmiles.ts | pino-pretty",
    "processSDF": "node src/scripts/processSDF.ts | pino-pretty",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "start": "node ./src/server.ts",
    "test": "npm run test-only && npm run check-types && npm run eslint && npm run prettier",
    "test-only": "vitest run"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/debug": "^4.1.12",
    "better-sqlite3": "^12.2.0",
    "bson": "^6.10.4",
    "fastify": "^5.5.0",
    "file-collection": "^5.1.1",
    "mass-tools": "^2.3.1",
    "openchemlib": "^9.7.0",
    "openchemlib-utils": "^8.3.1",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "piscina": "^5.1.3",
    "postgrator": "^8.0.0",
    "sdf-parser": "^7.0.4"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/workerpool": "^6.4.7",
    "@vitest/coverage-v8": "^3.2.4",
    "@zakodium/tsconfig": "^1.0.2",
    "eslint": "^9.34.0",
    "eslint-config-cheminfo-typescript": "^19.0.0",
    "globals": "^16.3.0",
    "mf-parser": "^3.6.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/ocl-cache.git"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/ocl-cache/issues"
  },
  "homepage": "https://github.com/cheminfo/ocl-cache#readme",
  "volta": {
    "node": "24.6.0"
  }
}
